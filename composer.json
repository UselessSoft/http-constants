{
    "name": "useless-soft/http-constants",
    "description": "Useful set of HTTP constants.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael Moravec"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "~2.1.0",
        "phpunit/phpunit": "^7",
        "symfony/http-foundation": "^4"
    },
    "autoload": {
        "psr-4": {
            "UselessSoft\\HttpConstants\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "UselessSoft\\HttpConstants\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    }
}
